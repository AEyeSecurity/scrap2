AGENT_BASE_URL=https://agents.reydeases.com
AGENT_USERNAME=your_user
AGENT_PASSWORD=your_password

# Optional: comma separated API endpoints captured from traffic.
# Supports {fromDate} and {toDate} placeholders.
# Example: /api/agent_admin/report?from={fromDate}&to={toDate}
AGENT_API_ENDPOINTS=

# Optional login settings
AGENT_LOGIN_PATH=/login
AGENT_USERNAME_SELECTORS=input[name="login"],input[name="username"],input[autocomplete="username"],input[type="text"]
AGENT_PASSWORD_SELECTORS=input[name="password"],input[type="password"]
AGENT_SUBMIT_SELECTORS=button[type="submit"],button:has-text("Log in"),button:has-text("Login")
AGENT_SUCCESS_SELECTOR=
AGENT_ERROR_SELECTOR=

# Scraper defaults
SCRAPER_DEFAULT_HEADLESS=false
SCRAPER_DEBUG=false
SCRAPER_TIMEOUT_MS=30000
SCRAPER_RETRIES=2
SCRAPER_CONCURRENCY=4
SCRAPER_BLOCK_RESOURCES=true
SCRAPER_REUSE_SESSION=true
SCRAPER_OUTPUT_DIR=./out
SCRAPER_ARTIFACTS_DIR=./artifacts
SCRAPER_LOG_LEVEL=info
SCRAPER_FROM_DATE=
SCRAPER_TO_DATE=
SCRAPER_MAX_PAGES=100

# Login API server defaults
API_HOST=127.0.0.1
API_PORT=3000
API_LOGIN_CONCURRENCY=3
API_JOB_TTL_MINUTES=60
